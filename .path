# Add sublime text binary
#SUBLIMEPATH="/Applications/Sublime Text 3.app/Contents/SharedSupport";
#PATH=$SUBLIMEPATH/bin:$PATH;

# Add Visual Studio Code binary
# This is handled via symlink into ~/bin
#VSCODEPATH="/Applications/Visual Studio Code M1.app/Contents/Resources/app";
#PATH=$VSCODEPATH/bin:$PATH;

# Original little snitch path
# This is added by '/etc/paths.d/at.obdev.littlesnitch'
# and that file needs to be removed on every update from LS
# The files are added via symlink in ~~/bin 
# /Applications/Little Snitch.app/Contents/Components

# Add Zig installation
#PATH="${HOME}/dev/ziglang/current:$PATH";

# Add x86_64 brew directories
PATH="/usr/local/bin:/usr/local/sbin:$PATH";
# Add aarch64 brew directories
PATH="/opt/homebrew/bin:/opt/homebrew/sbin:$PATH";

# Add x86_64 cargo binaries
PATH="$HOME/.x86_64/.cargo/bin:$PATH";
# Add aarch64 cargo binaries
PATH="$HOME/.cargo/bin:$PATH";

# Add ~/bin
PATH="$HOME/bin:$PATH";

export PATH;
